{
  "expo": {
    "name": "CalorieVision",
    "slug": "calorie-vision",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#101828"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#101828"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    }
  }
}
